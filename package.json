{
  "name": "warntf",
  "version": "0.1.0",
  "description": "",
  "repository": "pi0/warning.wtf",
  "license": "MIT",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./setup": {
      "types": "./dist/index.d.ts",
      "import": "./dist/setup.mjs",
      "require": "./dist/setup.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "play": "jiti playground",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "changelogen": "^0.6.2",
    "eslint": "^8.57.1",
    "eslint-config-unjs": "^0.6.2",
    "jiti": "^1.21.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "unbuild": "^2.0.0"
  },
  "packageManager": "pnpm@8.15.9"
}
